<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/spiderman1"
    tools:context=".Integrantes">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <Space
            android:layout_width="match_parent"
            android:layout_height="85dp" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="match_parent"
            android:layout_height="64dp"
            android:fontFamily="monospace"
            android:text="Equipo"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="34sp" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="match_parent"
            android:layout_height="64dp"
            android:fontFamily="sans-serif-light"
            android:text="Color 6"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="34sp" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="23dp" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="match_parent"
            android:layout_height="64dp"
            android:fontFamily="monospace"
            android:gravity="center_vertical"
            android:text="Integrantes"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="34sp" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:fontFamily="sans-serif-light"
            android:gravity="center_vertical"
            android:text="Adrian Flores Macias"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="30sp" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:fontFamily="sans-serif-light"
            android:gravity="center_vertical"
            android:text="Brayan Alberto Godinez Flores"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="30sp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:fontFamily="sans-serif-light"
            android:gravity="center_vertical"
            android:text="Yolanda Lopez Gomez"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="30sp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:fontFamily="sans-serif-light"
            android:gravity="center_vertical"
            android:text="Ivan Alejandro Mendoza Rivera"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="30sp" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="21dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <Space
                android:layout_width="99dp"
                android:layout_height="78dp"
                android:layout_weight="0" />

            <Button
                android:id="@+id/button"
                android:layout_width="210dp"
                android:layout_height="75dp"
                android:gravity="center|center_vertical"
                android:text="Iniciar App" />
        </LinearLayout>

    </LinearLayout>
</FrameLayout>